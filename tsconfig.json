{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": false,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "baseUrl": ".",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["es6", "ESNext", "dom"],
    "types": ["vite/client","vite-plugin-svg-icons/client"],
    "paths": {
      "@/*": ["src/*"],
      "/#/*": ["types/*"]
    },
    "noImplicitAny": false,
    "noImplicitThis": true
  },
  "noUnusedLocals": false,
  "include": ["*.d.ts","src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "types/**/*"],
  "exclude": ["node_modules", "dist", "**/*.js"]
}
